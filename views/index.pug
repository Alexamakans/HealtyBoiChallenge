html
    head
        title= title
        link(rel="stylesheet", type="text/css", href="/css/main.css")
        link(href="https://fonts.googleapis.com/css?family=Press+Start+2P", rel="stylesheet")
        link(href="https://unpkg.com/nes.css/css/nes.css", rel="stylesheet")
        script(type="text/javascript" src="/js/shader.js")
    body
        h1=" HealtyBoi Challenge"
        div(class="container")
            each account in accounts
                form(class="card", method="POST")
                    if account.useShaderForProfilePicture
                        canvas(class="profile-picture-canvas" id=`profilePictureCanvas${account.id}`)
                    else
                        img(src="data:image/jpeg;base64,")
                    div(class="heart")
                    a(href="/shader" style="color: rgb(20, 170, 255);")
                      h2 #{account.player.name}
                    h3 #{account.player.xp}
                    input(name="id", value=account.id, hidden)
                    button +

